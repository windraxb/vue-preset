{
  "useConfigFiles": true,
  "packageManager": "yarn",
  "useTaobaoRegistry": true,
  "presets": {
    "preset-config-js": {
      "useConfigFiles": true,
      "plugins": {
        "@vue/cli-plugin-babel": {},
        "@vue/cli-plugin-router": {
          "historyMode": false
        },
        "@vue/cli-plugin-vuex": {},
        "@vue/cli-plugin-eslint": {
          "config": "standard",
          "lintOn": ["save"]
        }
      },
      "vueVersion": "2",
      "cssPreprocessor": "less"
    }
  }
}